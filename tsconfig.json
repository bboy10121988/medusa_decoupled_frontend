{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "exactOptionalPropertyTypes": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@lib/config": [
        "lib/config"
      ],
      "@lib/sanity": [
        "lib/sanity"
      ],
      "@lib/seo": [
        "lib/seo"
      ],
      "@lib/medusa-publishable-key": [
        "lib/medusa-publishable-key"
      ],
      "@lib/store-name": [
        "lib/store-name"
      ],
      "@lib/types": [
        "lib/types/*",
        "types/*"
      ],
      "@lib/data": [
        "lib/data/*"
      ],
      "@lib/util": [
        "lib/util/*"
      ],
      "@lib/hooks": [
        "lib/hooks/*"
      ],
      "@lib/context": [
        "lib/context/*"
      ],
      "@lib/sanity-utils": [
        "lib/sanity-utils/*"
      ],
      "@modules/*": [
        "modules/*"
      ],
      "@modules/common": [
        "modules/common/*"
      ],
      "@modules/store": [
        "modules/store/*"
      ],
      "@modules/skeletons": [
        "modules/skeletons/*"
      ],
      "@modules/products": [
        "modules/products/*"
      ],
      "@modules/order": [
        "modules/order/*"
      ],
      "@modules/layout": [
        "modules/layout/*"
      ],
      "@modules/cart": [
        "modules/cart/*"
      ],
      "@modules/checkout": [
        "modules/checkout/*"
      ],
      "@modules/account": [
        "modules/account/*"
      ],
      "@modules/home": [
        "modules/home/*"
      ],
      "@modules/blog": [
        "modules/blog/*"
      ],
      "@modules/collections": [
        "modules/collections/*"
      ],
      "@modules/categories": [
        "modules/categories/*"
      ],
      "@modules/affiliate": [
        "modules/affiliate/*"
      ],
      "@modules/affiliate-admin": [
        "modules/affiliate-admin/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@sanity/types": [
        "../schemas/*",
        "types/*"
      ],
      "types": [
        "types/*",
        "../types/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next",
    ".nyc_output",
    "coverage",
    "jest-coverage",
    "tests",
    "playwright.config.ts",
    "scripts"
  ]
}